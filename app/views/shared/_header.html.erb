<%= link_to products_path do %>
  <h1>Sell it</h1>
<% end %>
<%= link_to 'Add Product', new_product_path if Current.user %>
<%= link_to 'Categories', categories_path if Current.user&.admin? %>
<%= link_to 'Log in', new_session_path unless Current.user %>
<%= link_to 'Sign up', new_user_path unless Current.user %>
<%= link_to 'Log out', session_path(Current.user.id), data: {
  turbo_method: :delete,
  turbo_confirm: 'Are you sure to log out?'
} if Current.user %>